() => {\n                setError(\"Failed to update account\")\n            })\n            .finally(() => {\n                setLoading(false)\n            })\n        setLoading(false)\n    }\n\n    return (\n        <>\n            <Form >\n                <Form.Group id=\"email\">\n                    <Form.Label>Email</Form.Label>\n                    <Form.Control\n                        type=\"email\"\n                        disabled={true}\n                        defaultValue={currentUser.email}\n                    />\n                </Form.Group>\n                <Form.Group >\n                    <Form.Label>Name</Form.Label>\n                    <Form.Control\n                        id=\"name\"\n                        disabled={true}\n                    //defaultValue={props.value.name}\n                    />\n                </Form.Group>\n                <Form.Group >\n                    <Form.Label>Surname</Form.Label>\n                    <Form.Control\n                        id=\"surname\"\n                        disabled={true}\n                    //defaultValue={props.value.surname}\n                    />\n                </Form.Group>\n                <Form.Group >\n                    <Form.Label>Address</Form.Label>\n                    <Form.Control\n                        id=\"address\"\n                        //defaultValue={props.value.address}\n                        ref={addressRef}\n                    />\n                </Form.Group>\n                <Form.Group >\n                    <Form.Label>City</Form.Label>\n                    <Form.Control\n                        id=\"city\"\n                        //defaultValue={props.value.city}\n                        ref={cityRef}\n                    />\n                </Form.Group>\n                <Form.Group >\n                    <Form.Label>Zip code</Form.Label>\n                    <Form.Control\n                        id=\"zipCode\"\n                        //defaultValue={props.value.zipCode}\n                        ref={zipCodeRef}\n                    />\n                </Form.Group>\n                <Form.Group id=\"password\">\n                    <Form.Label>Password</Form.Label>\n                    <Form.Control\n                        type=\"password\"\n                        ref={passwordRef}\n                        placeholder=\"Leave blank to keep the same\"\n                    />\n                </Form.Group>\n                <Form.Group id=\"password-confirm\">\n                    <Form.Label>Password Confirmation</Form.Label>\n                    <Form.Control\n                        type=\"password\"\n                        ref={passwordConfirmRef}\n                        placeholder=\"Leave blank to keep the same\"\n                    />\n                </Form.Group>\n                {error && <Alert variant=\"danger\">{error}</Alert>}\n                <Button onClick={() => handleSubmit()} disabled={loading} className=\"w-100\" >{/*={() => handleUpdate(currentUser,addressRef.current.value, cityRef.current.value, zipCodeRef.current.value, operations,passwordRef,passwordConfirmRef)}*/}\n                Update\n            </Button>\n            </Form>\n        </>\n    )\n}\nexport default UpdateForm;","/Users/samueledallospedale/Tecnologie_internet/stockx/src/components/Portfolio.js",["220","221","222","223","224","225","226","227","228","229","230","231","232"],"/Users/samueledallospedale/Tecnologie_internet/stockx/src/components/StocksHeld.js",["233","234","235","236","237","238","239","240","241"],"/Users/samueledallospedale/Tecnologie_internet/stockx/src/components/StocksHeldRow.js",["242","243","244","245","246","247","248","249","250"],"/Users/samueledallospedale/Tecnologie_internet/stockx/src/components/ClosePosition.js",[],"/Users/samueledallospedale/Tecnologie_internet/stockx/src/stockAPIPortfolio.js",[],"/Users/samueledallospedale/Tecnologie_internet/stockx/src/components/Credit.